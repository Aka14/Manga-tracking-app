{
  "manifest_version": 3,
  "name": "Mangapill bookmark extension",
  "icons": {
    "16": "/assets/squirtle1.png",
    "32": "/assets/squirtle1.png",
    "48": "/assets/squirtle1.png",
    "128": "/assets/squirtle1.png"
  },
  "version": "1.0.0",
  "background": {
    "service_worker": "service_worker.js"
  },
  "action": { },
  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": ["http://127.0.0.1:8000/*","*://*.mangapill.com/chapters/*"]
}
